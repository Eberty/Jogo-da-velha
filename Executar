gcc -O3 -std=c99 -Wall -pedantic -c torneio.c -o torneio.o
gcc -O3 -std=c99 -Wall -pedantic -c velha.c -o velha.o
gcc -O3 objetos.o erros.o interface.o estrategia.o torneio.o velha.o -o velha